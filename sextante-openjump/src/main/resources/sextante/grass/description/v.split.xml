<?xml version="1.0" encoding="CP1252"?>
<!DOCTYPE task SYSTEM "grass-interface.dtd">
<task name="v.split">
	<description>
		Split lines to shorter segments.
	</description>
	<keywords>
		vectorial, geometría
	</keywords>
	<parameter name="input" type="string" required="yes" multiple="no">
		<description>
			Nombre del mapa vectorial de entrada.
		</description>
		<keydesc>
			<item order="1">name</item>
		</keydesc>
		<gisprompt age="old" element="vector" prompt="vector" />
	</parameter>
	<parameter name="output" type="string" required="yes" multiple="no">
		<description>
			Nombre para el mapa vectorial de salida.
		</description>
		<keydesc>
			<item order="1">name</item>
		</keydesc>
		<gisprompt age="new" element="vector" prompt="vector" />
	</parameter>
	<parameter name="length" type="float" required="no" multiple="no">
		<description>
			Maximum segment length.
		</description>
	</parameter>
	<parameter name="vertices" type="integer" required="no" multiple="no">
		<description>
			Maximum number of vertices in segment.
		</description>
	</parameter>
	<flag name="overwrite">
		<description>
			Permite a los archivos de salida sobrescribir los archivos existentes.
		</description>
	</flag>
	<flag name="verbose">
		<description>
			Salida detallada del módulo.
		</description>
	</flag>
	<flag name="quiet">
		<description>
			Salida del módulo "silenciosa".
		</description>
	</flag>
</task>
