<?xml version="1.0" encoding="CP1252"?>
<!DOCTYPE task SYSTEM "grass-interface.dtd">
<task name="r.bitpattern">
	<description>
		Compara patrones de bit con un mapa ráster.
	</description>
	<keywords>
		ráster
	</keywords>
	<parameter name="input" type="string" required="yes" multiple="no">
		<description>
			Nombre del mapa raster de entrada.
		</description>
		<keydesc>
			<item order="1">name</item>
		</keydesc>
		<gisprompt age="old" element="cell" prompt="raster" />
	</parameter>
	<parameter name="output" type="string" required="yes" multiple="no">
		<description>
			Nombre del mapa raster de salida.
		</description>
		<keydesc>
			<item order="1">name</item>
		</keydesc>
		<gisprompt age="new" element="cell" prompt="raster" />
	</parameter>
	<parameter name="pattern" type="integer" required="yes" multiple="no">
		<description>
			Posición(es) de patrón de bit
		</description>
	</parameter>
	<parameter name="patval" type="integer" required="yes" multiple="no">
		<description>
			Valor de patrón de bit
		</description>
	</parameter>
	<flag name="q">
		<description>
			Silencioso
		</description>
	</flag>
	<flag name="overwrite">
		<description>
			Permite a los archivos de salida sobrescribir los archivos existentes.
		</description>
	</flag>
	<flag name="verbose">
		<description>
			Salida detallada del módulo.
		</description>
	</flag>
	<flag name="quiet">
		<description>
			Salida del módulo "silenciosa".
		</description>
	</flag>
</task>
