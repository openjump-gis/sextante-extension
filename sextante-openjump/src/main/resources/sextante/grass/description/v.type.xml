<?xml version="1.0" encoding="CP1252"?>
<!DOCTYPE task SYSTEM "grass-interface.dtd">
<task name="v.type">
	<description>
		Cambiar el tipo de elementos geométricos.
	</description>
	<keywords>
		vectorial, geometría
	</keywords>
	<parameter name="input" type="string" required="yes" multiple="no">
		<description>
			Nombre del mapa vectorial de entrada.
		</description>
		<keydesc>
			<item order="1">name</item>
		</keydesc>
		<gisprompt age="old" element="vector" prompt="vector" />
	</parameter>
	<parameter name="output" type="string" required="yes" multiple="no">
		<description>
			Nombre para el mapa vectorial de salida.
		</description>
		<keydesc>
			<item order="1">name</item>
		</keydesc>
		<gisprompt age="new" element="vector" prompt="vector" />
	</parameter>
	<parameter name="type" type="string" required="no" multiple="yes">
		<label>
			Pares de tipos de entrada y salida separados por coma.
		</label>
		<description>
			&lt;input_type1&gt;,&lt;output_type1&gt;,&lt;input_type2&gt;,&lt;output_type2&gt;,...
		Ejemplo1: línea,contorno
		Ejemplo2: línea,contorno,punto,centroide
		</description>
		<default>
			line,boundary,point,centroid
		</default>
		<values>
			<value>
				<name>point</name>
			</value>
			<value>
				<name>line</name>
			</value>
			<value>
				<name>boundary</name>
			</value>
			<value>
				<name>centroid</name>
			</value>
			<value>
				<name>face</name>
			</value>
			<value>
				<name>kernel</name>
			</value>
		</values>
	</parameter>
	<flag name="overwrite">
		<description>
			Permite a los archivos de salida sobrescribir los archivos existentes.
		</description>
	</flag>
	<flag name="verbose">
		<description>
			Salida detallada del módulo.
		</description>
	</flag>
	<flag name="quiet">
		<description>
			Salida del módulo "silenciosa".
		</description>
	</flag>
</task>
