<?xml version="1.0" encoding="CP1252"?>
<!DOCTYPE task SYSTEM "grass-interface.dtd">
<task name="r.out.mat">
	<description>
		Exporta un ráster de GRASS a un archivo MAT binario.
	</description>
	<keywords>
		ráster, exportar
	</keywords>
	<parameter name="input" type="string" required="yes" multiple="no">
		<description>
			Nombre del mapa raster de entrada.
		</description>
		<keydesc>
			<item order="1">name</item>
		</keydesc>
		<gisprompt age="old" element="cell" prompt="raster" />
	</parameter>
	<parameter name="output" type="string" required="yes" multiple="no">
		<description>
			Nombre para el archivo MAT binario de salida
		</description>
		<gisprompt age="new_file" element="file" prompt="output" />
	</parameter>
	<flag name="v">
		<description>
			Modo verboso
		</description>
	</flag>
	<flag name="verbose">
		<description>
			Salida detallada del módulo.
		</description>
	</flag>
	<flag name="quiet">
		<description>
			Salida del módulo "silenciosa".
		</description>
	</flag>
</task>
