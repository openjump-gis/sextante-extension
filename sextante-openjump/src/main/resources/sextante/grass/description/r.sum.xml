<?xml version="1.0" encoding="CP1252"?>
<!DOCTYPE task SYSTEM "grass-interface.dtd">
<task name="r.sum">
	<description>
		Totaliza los valores de las celdas ráster.
	</description>
	<keywords>
		ráster
	</keywords>
	<parameter name="rast" type="string" required="yes" multiple="no">
		<description>
			Nombre del archivo de incidencia o de densidad.
		</description>
		<gisprompt age="old" element="cell" prompt="raster" />
	</parameter>
	<flag name="verbose">
		<description>
			Salida detallada del módulo.
		</description>
	</flag>
	<flag name="quiet">
		<description>
			Salida del módulo "silenciosa".
		</description>
	</flag>
</task>
