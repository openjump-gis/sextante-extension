<?xml version="1.0" encoding="CP1252"?>
<!DOCTYPE task SYSTEM "grass-interface.dtd">
<task name="v.in.geonames">
	<description>
		Imports geonames.org country files into a GRASS vector points map.
	</description>
	<keywords>
		vector, import, gazetteer
	</keywords>
	<parameter name="input" type="string" required="yes" multiple="no">
		<description>
			Uncompressed geonames file from (with .txt extension)
		</description>
		<keydesc>
			<item order="1">name</item>
		</keydesc>
		<gisprompt age="old_file" element="file" prompt="input" />
	</parameter>
	<parameter name="output" type="string" required="yes" multiple="no">
		<description>
			Nombre para el mapa vectorial de salida
		</description>
		<keydesc>
			<item order="1">name</item>
		</keydesc>
		<gisprompt age="new" element="vector" prompt="vector" />
	</parameter>
	<flag name="overwrite">
		<description>
			Permite a los archivos de salida sobrescribir los archivos existentes.
		</description>
	</flag>
	<flag name="verbose">
		<description>
			Salida detallada del módulo.
		</description>
	</flag>
	<flag name="quiet">
		<description>
			Salida del módulo "silenciosa".
		</description>
	</flag>
</task>
